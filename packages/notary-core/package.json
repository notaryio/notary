{
  "name": "notary",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "node dist",
    "dev": "nodemon -w src/ --exec babel-node src/index.js",
    "test": "npm run unit && npm run lint",
    "lint": "eslint ./src && eslint ./tests",
    "fixlint": "eslint --fix ./src && eslint --fix ./tests",
    "unit": "nyc npm run mocha && nyc report --reporter=text-lcov --silent > coverage.lcov",
    "mocha": "mocha --compilers js:babel-core/register --recursive --timeout 5000 tests"
  },
  "dependencies": {
    "ajv": "^5.0.1",
    "child-process-promise": "^2.2.1",
    "deep-diff": "^0.3.8",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "express-winston": "^2.0.0",
    "js-yaml": "^3.8.4",
    "lodash": "^4.17.4",
    "pretty-swag": "^0.1.100",
    "swagger-parser": "^4.0.0-beta.2",
    "verror": "^1.10.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "codecov": "^2.2.0",
    "eslint": "^3.15.0",
    "eslint-plugin-prettier": "^2.0.1",
    "lerna": "^2.2.0",
    "mocha": "^3.2.0",
    "mockery": "^2.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.3.2",
    "prettier": "^1.3.0",
    "sinon": "^2.2.0",
    "superagent": "^3.4.1"
  },
  "nyc": {
    "include": [
      "src/**"
    ]
  }
}
